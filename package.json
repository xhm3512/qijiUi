  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "build:dist": "father-build ./.fatherrc-component.ts",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "dep": "./beta.sh",
    "commit": "git-cz"
  },
  "config": {
    "validate-commit-msg": {}
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11",
    "not IE 10"
  ],
  "keywords": [
    "ant",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@types/classnames": "^2.3.1",
    "braft-editor": "^2.3.9",
    "classnames": "^2.2.6",
    "moment": "^2.29.3",
    "rc-checkbox": "^2.3.2",
    "rc-dialog": "^8.9.0",
    "rc-drawer": "~4.4.2",
    "rc-dropdown": "^4.0.1",
    "rc-input": "0.0.1-alpha.7",
    "rc-menu": "^9.8.2",
    "rc-motion": "^2.6.0",
    "rc-picker": "^2.6.10",
    "rc-select": "^14.1.5",
    "rc-tabs": "^11.16.0",
    "rc-tooltip": "^5.1.1",
    "rc-upload": "^4.3.4",
    "rc-util": "^5.21.5",
    "react-loadable": "^5.5.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "antd": "^4.16.3",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "dumi": "^1.1.30",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "father-build": "^1.17.2",
    "jsdom": "^16.7.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "yorkie": "^2.0.0"
  }
